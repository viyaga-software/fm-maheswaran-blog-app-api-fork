{
  "kind": "collectionType",
  "collectionName": "website_users",
  "info": {
    "singularName": "website-user",
    "pluralName": "website-users",
    "displayName": "Website Users",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "unique": true
    },
    "email": {
      "type": "email",
      "required": true,
      "maxLength": 255,
      "unique": true
    },
    "Password": {
      "type": "password",
      "minLength": 5,
      "required": true
    },
    "first_name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "last_name": {
      "type": "string",
      "maxLength": 255
    },
    "country": {
      "type": "string",
      "maxLength": 255
    },
    "prime_membership": {
      "type": "decimal",
      "default": 0
    },
    "user_status": {
      "type": "integer",
      "default": 1
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "website_user"
    }
  }
}
